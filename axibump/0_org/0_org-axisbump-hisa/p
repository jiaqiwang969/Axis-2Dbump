FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;


boundaryField
{
    inlet
    {
        type            zeroGradient;
//        type            fixedValue;
//        value           uniform 1e5;
    }


    outlet
    {
        type            fixedValue;
        value           $internalField;
        type            zeroGradient;
    }



    top
    {
        type            symmetry;
    }

    obstacle
    {
        type            zeroGradient;
    }


    upwall
    {
        type            symmetry;
    }

    downwall
    {
        type            symmetry;
    }








    front
    {
        type            wedge;
    }

    back
    {
        type            wedge;
    }
}

// ************************************************************************* //
