FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Ret;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 30.0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 800;
    }
    atmosphere
    {
        type            zeroGradient;
    }
    obstacle
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}

